                            Marla = _thisInner.find('.'+m).val() != '' ? (parseFloat(_thisInner.find('.'+m).val())*parseFloat($('#'+m+'UnitValue').val())*parseFloat($('#'+m+'Timesof').val()))/parseFloat($('#'+m+'DivideBy').val()) : 0.0;
                        }
                        if (_thisInner.find('.'+s).length > 0) {

