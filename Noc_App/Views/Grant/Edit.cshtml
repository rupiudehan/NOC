    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <form method="post" asp-action="KhasraSave" asp-controller="Grant">
            <div class="modal-header">
                <input type="hidden" id="id" name="id" />
                <input type="hidden" id="KId" name="KId" />
                <input type="hidden" id="UnitId" name="UnitId" />
                <h5 class="modal-title" id="exampleModalLabel"><label id="dvBranch">Add Khasra Details</label></h5>
                <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body" style="max-height: 500px;overflow-y:scroll;">
                @* <form class="user" enctype="multipart/form-data" asp-action="KhasraSave" method="post">                     *@
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <label for="UnitName" class="control-label">Unit of Site Area</label>
                                <input id="UnitName" readonly name="UnitName" class="form-control form-control-user" />
                            </div>
                            <div class="form-group">
                                <label for="KhasraNo" class="control-label">Khasra No.</label>
                                <input id="KhasraNo" name="KhasraNo" class="form-control form-control-user"  placeholder="Khasra No."/>
                                <span id="KhasraNo-validation" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label for="KanalOrBigha" class="control-label">Kanal/Bigha</label>
                                <input id="KanalOrBigha" name="KanalOrBigha" class="form-control form-control-user" placeholder="Kanal/Bigha"/>
                                <span id="KanalOrBigha-validation" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label for="MarlaOrBiswa" class="control-label">Marla/Biswa</label>
                                <input id="MarlaOrBiswa" name="MarlaOrBiswa" class="form-control form-control-user" placeholder="Marla/Biswa" />
                                <span id="MarlaOrBiswa-validation" class="text-danger"></span>
                            </div>
                            <div class="form-group">
